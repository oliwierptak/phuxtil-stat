{
    "name": "phuxtil/stat",
    "description": "Easy interface for output of 'stat' unix command",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^4",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Phuxtil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhuxtilTests\\Functional\\": "tests/Functional"
        }
    }
}
